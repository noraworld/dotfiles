[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[include]
	path = .gitconfig.local
[user]
	useConfigOnly = true
[color]
	ui = auto
[alias]
	up = !echo \"Pushing into branch $(git rev-parse --abbrev-ref HEAD)...\" && echo \"\" && git push -u origin $(git rev-parse --abbrev-ref HEAD)
	alias = !git config --global --list | grep --color=always \"alias\"
	count = !git log --oneline | wc -l
[ghq]
	root = ~/.ghq/src
