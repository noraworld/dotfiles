if [ "$(expr "$CURRENT_COMMAND" : '^[[:blank:]]*docker-compose[[:blank:]]*')" -eq 0 ] &&
   [ "$(expr "$CURRENT_COMMAND" : '^[[:blank:]]*sudo[[:blank:]]*docker-compose[[:blank:]]*')" -eq 0 ]
then
  \docker-compose "$@"
  return $?
fi

case $1 in
  up)
    __alarm "docker-compose up" "docker-server" "This command ignores stdin such as binding.pry"
    ;;
  *)
    command docker-compose "$@"
    ;;
esac
